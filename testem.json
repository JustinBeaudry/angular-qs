{
  "framework": "jasmine2",
  "src_files": [
    "lib/*.js",
    "test/*.js"
  ],
  "serve_files": [
    "node_modules/angular/angular.js",
    "node_modules/angular-mocks/angular-mocks.js",
    "dist/angular-qs.js",
    "test/*.js"
  ],
  "launch_in_dev": ["PhantomJS", "Chrome", "Firefox", "Safari"],
  "launch_in_ci": ["PhantomJS", "Firefox"],
  "before_tests": "npm run build"
}
